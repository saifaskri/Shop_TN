{% extends 'base.html.twig' %}

{% block title %}Creat Shop{% endblock %}
{% block stylesheets %}<link rel="stylesheet" href="{{asset('css/Shop.css')}}" >{% endblock %}
{% block body %}
<div class="container">
    <h1>Creat Your Own Shop</h1>
    {% for message in app.flashes('addShop') %}
                <div class="alert alert-danger text-center">
                    {{ message }}
                </div>
    {% endfor %}
    {{form_start(form)}}
        <input type="hidden" name="tokenShopAdd" value="{{ csrf_token('AddShopName') }}"/>
    {{form_end(form)}}
</div>
{% endblock %}
